	}

	private void setCantidadJugadores(int cantidadJugadores) {
		if(cantidadJugadores <= 14) {
		this.cantidadJugadores = cantidadJugadores;
		}
	}



	
	
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}

}
